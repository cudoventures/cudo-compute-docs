{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "CUDO Compute",
  "colors": {
    "primary": "#000",
    "light": "#FFF",
    "dark": "#000"
  },
  "background": {
    "color": {
      "light": "#F9F9F9",
      "dark": "#09090B"
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "openapi": {
              "source": "/api-reference/openapi.json",
              "directory": "api-reference"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://cudocompute.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://cudocompute.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@cudoventures.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://compute.cudo.org"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/Cudo_Compute",
      "github": "https://github.com/cudoventures",
      "linkedin": "https://linkedin.com/company/cudocompute"
    }
  }
}
