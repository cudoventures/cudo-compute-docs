{
    "name": "cudo-docs",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "description": "Documentation package",
    "license": "MIT",
    "scripts": {
        "dev": "pnpx mint dev",
        "sort:api": "node ./lib/sortTags.js -i ./docs/openapi.json -p",
        "gen:api": "node ./lib/genRefPages.js -i ./docs/openapi.json -o ./docs/output",
        "scrape:api": "npx @mintlify/scraping@latest openapi-file ./docs/openapi.json -o ./docs/output",
        "gen:help": "node ./lib/genHelpFromCsv.js -i ./lib/contentful-help-export.csv --update-nav ./docs.json"
    },
    "keywords": [],
    "author": "",
    "devDependencies": {
        "@types/node": "^24.7.0",
        "openapi-types": "^12.1.3",
        "csv-parse": "^5.5.6"
    }
}
